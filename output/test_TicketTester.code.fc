(int, int, int, int) $global_empty_winning_split() impure inline {
    return $WinningSplit$_constructor_n0_n1_n2_n3(0, 0, 0, 0);
}

(slice, int, int, (int, int, int, int), cell) $TicketTester$_contract_init(slice $owner, int $picks, int $prize_amount) impure inline_ref {
    var (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)) = (null(), 0, 0, null(), null());
    $self'owner = $owner;
    $self'picks = $picks;
    $self'prize_amount = $prize_amount;
    ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3) = $global_empty_winning_split();
    $self'used_indexes = null();
    return ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes);
}

int $WinningSplit$_fun_compute_prize_scale((int, int, int, int) $self, int $pick_number) impure inline {
    var (($self'n0, $self'n1, $self'n2, $self'n3)) = $self;
    var (($self'n0, $self'n1, $self'n2, $self'n3)) = $self;
    int $num = ($pick_number % 16);
    if (($num != $self'n0)) {
        return 0;
    }
    $pick_number = ($pick_number / 16);
    $num = ($pick_number % 16);
    if (($num != $self'n1)) {
        return 6750;
    }
    $pick_number = ($pick_number / 16);
    $num = ($pick_number % 16);
    if (($num != $self'n2)) {
        return 36000;
    }
    $pick_number = ($pick_number / 16);
    $num = ($pick_number % 16);
    if (($num != $self'n3)) {
        return 192000;
    }
    return 960000;
}

((slice, int, int, (int, int, int, int), cell), int) $TicketTester$_fun_compute_pick_number((slice, int, int, (int, int, int, int), cell) $self, int $index) impure inline {
    var (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)) = $self;
    int $hash = cell_hash(end_cell(store_uint(__tact_store_address(begin_cell(), my_address()), $index, 16)));
    return (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes), ($hash % 65536));
}

((slice, int, int, (int, int, int, int), cell), int) $TicketTester$_fun_compute_prize_scale((slice, int, int, (int, int, int, int), cell) $self, slice $index_payload) impure inline {
    var (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)) = $self;
    int $size = (slice_bits($index_payload) / 16);
    $self'picks = ($self'picks - $size);
    throw_unless(16022, ($self'picks >= 0));
    int $scale = 0;
    repeat ($size) {
        int $pick_index = $index_payload~load_uint(16);
        int $pick_used = __tact_dict_get_int_int($self'used_indexes, 257, $pick_index, 1);
        throw_unless(46002, null?($pick_used));
        $self'used_indexes~__tact_dict_set_int_int(257, $pick_index, true, 1);
        int $pick_number = ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)~$TicketTester$_fun_compute_pick_number($pick_index);
        $scale = ($scale + $WinningSplit$_fun_compute_prize_scale(($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $pick_number));
    }
    return (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes), $scale);
}

