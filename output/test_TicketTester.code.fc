(((slice, int, int, (int, int, int, int), cell)), ()) $TicketTester$_internal_binary_ComputePickNumber((slice, int, int, (int, int, int, int), cell) $self, (int, int) $msg) impure inline {
    var ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes) = $self;
    var ($msg'index, $msg'expected_number) = $msg;
    int $pick_number = (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)~$TicketTester$_fun_compute_pick_number($msg'index) % 65536);
    throw_unless(2221, ($pick_number == $msg'expected_number));
    return (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes), ());
}

(((slice, int, int, (int, int, int, int), cell)), ()) $TicketTester$_internal_binary_ComputePrizeAmount((slice, int, int, (int, int, int, int), cell) $self, (int, cell) $msg) impure inline {
    var ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes) = $self;
    var ($msg'expected_amount, $msg'index_payload) = $msg;
    int $prize_scale = ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)~$TicketTester$_fun_compute_prize_scale(begin_parse($msg'index_payload));
    int $prize_amount = (($self'prize_amount * $prize_scale) / 38400000);
    throw_unless(60803, ($prize_amount == $msg'expected_amount));
    return (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes), ());
}

(((slice, int, int, (int, int, int, int), cell)), ()) $TicketTester$_internal_binary_Deploy((slice, int, int, (int, int, int, int), cell) $self, (int) $deploy) impure inline {
    var ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes) = $self;
    var ($deploy'queryId) = $deploy;
    ($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes)~$TicketTester$_fun_notify($DeployOk$_store_cell($DeployOk$_constructor_queryId($deploy'queryId)));
    return (($self'owner, $self'picks, $self'prize_amount, ($self'winning_split'n0, $self'winning_split'n1, $self'winning_split'n2, $self'winning_split'n3), $self'used_indexes), ());
